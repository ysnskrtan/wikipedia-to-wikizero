{
  "manifest_version": 2,

  "name": "WikiPedia to WikiZero",
  "description": "This extension allows the user to access the wikipedia from restricted countries.",
  "version": "0.1",

  "browser_action": {
    "default_icon": {
		"16": "icon16.png",
		"24": "icon24.png",
		"32": "icon32.png"
	},
	"default_title": "WikiPedia to WikiZero",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
    "tabs"
  ]
}